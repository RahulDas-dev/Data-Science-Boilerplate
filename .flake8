[flake8]
ignore = D103, D401, PT009, E731, C901
exclude = .git,
        .github,
        .vscode,
        __pycache__,
        data,
        model,
        .gitignore,
        .pre-commit-config.yaml,
        .python-version,
        README.md, 
        requiremwnts_dev.txt,
        requiremwnts.txt
spellcheck-targets = comments
max-complexity = 10
max-line-length = 120
inline-quotes = double
[isort]
line_length=120
default_section = THIRDPARTY
sections = STDLIB,THIRDPARTY,FIRSTPARTY,LOCALFOLDER
profile=black